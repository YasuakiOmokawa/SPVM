
all:
	cc -c -DSPVM_DEBUG_DUMP -DSPVM_DEBUG_OBJECT_COUNT -fno-strict-aliasing -pipe -fstack-protector -I/usr/local/include -D_LARGEFILE_SOURCE -D_FILE_OFFSET_BITS=64 -std=c99  -Ilib/SPVM/Builder/include -Wall -Wextra -Wno-unused-label -Wno-unused-function -Wno-unused-label -Wno-unused-parameter -Wno-unused-variable -Wno-missing-field-initializers -O0 -g -o solo/main.o solo/main.c
	cc  -o solo/main -lm -o solo/main solo/main.o lib/SPVM/Builder/src/spvm_allow.o lib/SPVM/Builder/src/spvm_array_field_access.o lib/SPVM/Builder/src/spvm_basic_type.o lib/SPVM/Builder/src/spvm_block.o lib/SPVM/Builder/src/spvm_call_sub.o lib/SPVM/Builder/src/spvm_case_info.o lib/SPVM/Builder/src/spvm_compiler.o lib/SPVM/Builder/src/spvm_compiler_allocator.o lib/SPVM/Builder/src/spvm_constant.o lib/SPVM/Builder/src/spvm_constant_pool.o lib/SPVM/Builder/src/spvm_csource_builder_exe.o lib/SPVM/Builder/src/spvm_csource_builder_precompile.o lib/SPVM/Builder/src/spvm_descriptor.o lib/SPVM/Builder/src/spvm_dumper.o lib/SPVM/Builder/src/spvm_enumeration.o lib/SPVM/Builder/src/spvm_enumeration_value.o lib/SPVM/Builder/src/spvm_field.o lib/SPVM/Builder/src/spvm_field_access.o lib/SPVM/Builder/src/spvm_hash.o lib/SPVM/Builder/src/spvm_hash_func.o lib/SPVM/Builder/src/spvm_list.o lib/SPVM/Builder/src/spvm_my.o lib/SPVM/Builder/src/spvm_op.o lib/SPVM/Builder/src/spvm_op_checker.o lib/SPVM/Builder/src/spvm_opcode.o lib/SPVM/Builder/src/spvm_opcode_array.o lib/SPVM/Builder/src/spvm_opcode_builder.o lib/SPVM/Builder/src/spvm_package.o lib/SPVM/Builder/src/spvm_package_var.o lib/SPVM/Builder/src/spvm_package_var_access.o lib/SPVM/Builder/src/spvm_portable.o lib/SPVM/Builder/src/spvm_runtime_api.o lib/SPVM/Builder/src/spvm_string_buffer.o lib/SPVM/Builder/src/spvm_sub.o lib/SPVM/Builder/src/spvm_switch_info.o lib/SPVM/Builder/src/spvm_toke.o lib/SPVM/Builder/src/spvm_type.o lib/SPVM/Builder/src/spvm_unicode.o lib/SPVM/Builder/src/spvm_use.o lib/SPVM/Builder/src/spvm_utf8proc.o lib/SPVM/Builder/src/spvm_util_allocator.o lib/SPVM/Builder/src/spvm_var.o lib/SPVM/Builder/src/spvm_yacc.o lib/SPVM/Builder/src/spvm_yacc_util.o blib/lib/SPVM/CORE.so blib/lib/SPVM/CORE.precompile.so blib/lib/SPVM/FileHandle.so

test:
	cc -c -DSPVM_DEBUG_DUMP -DSPVM_DEBUG_OBJECT_COUNT -fno-strict-aliasing -pipe -fstack-protector -I/usr/local/include -D_LARGEFILE_SOURCE -D_FILE_OFFSET_BITS=64 -std=c99  -Ilib/SPVM/Builder/include -Wall -Wextra -Wno-unused-label -Wno-unused-function -Wno-unused-label -Wno-unused-parameter -Wno-unused-variable -Wno-missing-field-initializers -O0 -g -o solo/t/spvm_t_list solo/t/spvm_t_list.c spvm_list.o spvm_util_allocator.o
	solo/t/spvm_t_list
	cc -c -DSPVM_DEBUG_DUMP -DSPVM_DEBUG_OBJECT_COUNT -fno-strict-aliasing -pipe -fstack-protector -I/usr/local/include -D_LARGEFILE_SOURCE -D_FILE_OFFSET_BITS=64 -std=c99  -Ilib/SPVM/Builder/include -Wall -Wextra -Wno-unused-label -Wno-unused-function -Wno-unused-label -Wno-unused-parameter -Wno-unused-variable -Wno-missing-field-initializers -O0 -g -o solo/t/spvm_t_hash solo/t/spvm_t_hash.c spvm_hash.o spvm_util_allocator.o spvm_hash_func.o
	solo/t/spvm_t_hash

clean:
	rm -rf solo/main

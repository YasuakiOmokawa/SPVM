  sub call_sub_args_int($var1 : int, $var2 : int, $var3 : int) : int {
    if ($var1 == 1) {
      if ($var2 == 2147483647) {
        if ($var3 == -2147483647) {
          return 1;
        }
      }
    }
    
    return 0;
  }
  
  sub call_sub_args_long($var1 : long, $var2 : long, $var3 : long) : int {

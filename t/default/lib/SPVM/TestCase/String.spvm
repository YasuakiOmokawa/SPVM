package TestCase::String {
  use Std::Arrays;
  
  sub basic : int () {
    # get_bytes
    my $get_bytes_ok = 0;
    {
      my $string1 = "abcde";
      
      my $bytes = $string1->get_bytes;
      my $bytes_expected = new byte[] { 'a', 'b', 'c', 'd', 'e' };
      if (Std::Arrays->equals_byte($bytes, $bytes_expected)) {
        $get_bytes_ok = 1;
      }
    }
    
    if ($get_bytes_ok) {
      return 1;
    }
    
    return 0;
  }
}

package TestCase::Short {
  use SPVM::Short;

  sub basic : int () {
    my $obj_value = SPVM::Short->new(3);
    my $value1 = $obj_value->get_value;
    
    $obj_value->set_value(5);
    my $value2 = $obj_value->get_value;
    
    if ($value1 == 3 && $value2 == 5) {
      return 1;
    }
    
    return 0;
  }
}

  sub subtract_int_int : int () {
    my $total1 = 2147483646 - 1;
    
    unless ($total1 == 2147483645) {
      return 0;
    }
    
    unless ($total1 isa int) {
      return 0;
    }

    return 1;
  }

  sub subtract_int_float : int () {

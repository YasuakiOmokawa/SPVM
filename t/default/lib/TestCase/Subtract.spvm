  sub subtract_float_inf_plus_value : int () {
    my $total = INFINITYF() - 1;
    
    unless(isinf($total)) {
      return 0;
    }

    unless($total > 0) {
      return 0;
    }
    
    unless($total isa float) {
      return 0;
    }
    
    return 1;

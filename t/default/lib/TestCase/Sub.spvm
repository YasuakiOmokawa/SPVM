  sub return_value_byte_sub : TestCase::Point_b3 () {
    my $value : TestCase::Point_b3;
    $value->{x} = INT8_MIN();
    $value->{y} = 1;
    $value->{z} = 2;
    
    return $value;
  }
  
  sub return_value_byte : int () {
    my $value : TestCase::Point_b3 = return_value_byte_sub();
    
    if ($value->{x} == INT8_MIN()) {
      if ($value->{y} == 1) {
        if ($value->{z} == 2) {
          return 1;
        }
      }
    }
    
    return 0;
  }
  

  sub call_sub_nest : int () {
    my $total = call_sub_nest_sum(call_sub_nest_sum(1, 2), call_sub_nest_sum(3, 4));
    
    if ($total == 10) {
      return 1;
    }
    
    return 0;
  }
  sub call_sub_nest_sum : int ($num1 : int, $num2 : int) {

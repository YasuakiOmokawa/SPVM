package TestCase::Add {

  sub add_byte_byte : int () {
    my $total = (byte)126 + (byte)127;
    
    unless ($total == 253) {
      return 0;
    }
    
    unless ($total isa int) {
      return 0;
    }
    
    return 1;
  }

  sub add_short_short : int () {
    my $total = (short)32766 + (short)32767;
    
    unless ($total == 65533) {

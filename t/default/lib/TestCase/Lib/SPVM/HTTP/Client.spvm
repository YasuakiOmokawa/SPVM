package TestCase::Lib::SPVM::HTTP::Client {
  use SPVM::HTTP::Client;

  sub test_basic : int () {
    my $client = SPVM::HTTP::Client->new;
    $client->{default_headers} = hash([(object)
      "X-Foo" => "Bar",
    ]);
    $client->request("GET", "http://www.google.com/?q=spvm", undef);
    return 1;
  }
}

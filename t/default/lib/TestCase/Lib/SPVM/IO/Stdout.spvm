package TestCase::Lib::SPVM::IO::Stdout {
  use SPVM::IO::Stdout;

  sub test_print : int () {
    my $stdout = SPVM::IO::Stdout->new;
    $stdout->print("Hello\n");
    
    return 1;
  }

  sub test_set_binmode : int () {
    my $stdout = SPVM::IO::Stdout->new;
    
    $stdout->set_binmode(0);
    
    return 1;
  }
}

package TestCase::Simple {
  has x : int;
  has y : int;
  
  our $FOO : int;

  precompile sub get_x : int ($self : self) {
    return $self->{x};
  }

  precompile sub set_x : void ($self : self, $x : int) {
    $self->{x} = $x;
  }
  
  precompile sub new : TestCase::Minimal () {
    return new TestCase::Minimal;
  }
}

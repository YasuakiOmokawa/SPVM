  use Std::Number::Util;

  jit sub byte_constant : int () {
    if ((int)Std::Number::Util->BYTE_MIN_VALUE() == (int)(byte)-128) {
      if ((int)Std::Number::Util->BYTE_MAX_VALUE() == (int)(byte)127) {
        if (Std::Number::Util->BYTE_BYTES() == 1) {
          if (Std::Number::Util->BYTE_SIZE() == 8) {
            return 1;
          }
        }
      }
    }
    
    return 0;
  }


  jit sub float_is_infinite: int () {
    if (Std::Number::Util->float_is_infinite(Std::Number::Util->FLOAT_POSITIVE_INFINITY())) {
      if (Std::Number::Util->float_is_infinite(Std::Number::Util->FLOAT_NEGATIVE_INFINITY())) {

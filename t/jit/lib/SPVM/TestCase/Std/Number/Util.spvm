  }
  
  jit sub float_int_bits_to_float_nan_first_condition: int () {
    
    if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0x7F800000)) == 0x7F800000) {
      if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0x7F800001)) == 0x7FC00000) {
        if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0x7F800002)) == 0x7FC00000) {
          if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0x7FFFFFFF)) == 0x7FC00000) {
            if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0x80000000)) == 0x80000000) {
              return 1;
            }
          }
        }
      }
    }
    
    return 0;
  }

  jit sub float_int_bits_to_float_nan_first_condition_is_nan: int () {
    
    if (!Std::Number::Util->float_is_nan(Std::Number::Util->float_int_bits_to_float(0x7F800000))) {
      if (Std::Number::Util->float_is_nan(Std::Number::Util->float_int_bits_to_float(0x7F800001))) {
        if (Std::Number::Util->float_is_nan(Std::Number::Util->float_int_bits_to_float(0x7F800002))) {
          if (Std::Number::Util->float_is_nan(Std::Number::Util->float_int_bits_to_float(0x7FFFFFFF))) {
            if (!Std::Number::Util->float_is_nan(Std::Number::Util->float_int_bits_to_float(0x80000000))) {

      if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0xFF800001)) == 0x7FC00000) {
        if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0xFF800002)) == 0x7FC00000) {
          if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0xFFFFFFFF)) == 0x7FC00000) {
            if (Std::Number::Util->float_float_to_raw_int_bits(Std::Number::Util->float_int_bits_to_float(0x00000000)) == 0x00000000) {

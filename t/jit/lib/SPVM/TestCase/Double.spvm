package TestCase::Double {
  use Double;

  sub basic : int () {
    my $obj_value = Double->new(0.25);
    my $value1 = $obj_value->get_value;
    
    $obj_value->set_value(0.5);
    my $value2 = $obj_value->get_value;
    
    if ($value1 == 0.25 && $value2 == 0.5) {
      return 1;
    }
    
    return 0;
  }
}

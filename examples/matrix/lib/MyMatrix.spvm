package MyMatrix {
  use SPVM::Matrix::Float;
  
  sub add_float_matrix : void () {
    # 1 3 5
    # 2 4 6
    my $mat1 = SPVM::Matrix::Float->new([1f, 2f, 3f, 4f, 5f, 6f], 2, 3);
    
    # 2 4 6
    # 3 5 7
    my $mat2 = SPVM::Matrix::Float->new([2f, 3f, 4f, 5f, 6f, 7f], 2, 3);
    
    my $mat3 = $mat1->add($mat2);
    
    print $mat3->str;
  }
}

package SPVM::Double {
  has value : private double;
  
  sub new : SPVM::Double ($value : double) {
    my $self = new SPVM::Double;
    
    $self->{value} = $value;
    
    return $self;
  }
  
  sub get_value : double ($self : self) {
    return $self->{value};
  }

  sub set_value : void ($self : self, $value : double) {
    $self->{value} = $value;
  }
}

package Byte {
  has value : byte;
  
  sub new : Byte ($value : byte) {
    my $self = new Byte;
    
    $self->{value} = $value;
    
    return $self;
  }
  
  sub get_value : byte ($self : self) {
    return $self->{value};
  }
  
  sub set_value : void ($self : self, $value : byte) {
    $self->{value} = $value;
  }
}

package SPVM::Byte {
  has value : private byte;
  
  sub new : SPVM::Byte ($value : byte) {
    my $self = new SPVM::Byte;
    
    $self->{value} = $value;
    
    return $self;
  }
  
  sub get_value : byte ($self : self) {
    return $self->{value};
  }
  
  sub set_value : void ($self : self, $value : byte) {
    $self->{value} = $value;
  }
}

package SPVM::Short {
  has value : private short;
  
  sub new : SPVM::Short ($value : short) {
    my $self = new SPVM::Short;
    
    $self->{value} = $value;
    
    return $self;
  }
  
  sub get_value : short ($self : self) {
    return $self->{value};
  }
  
  sub set_value : void ($self : self, $value : short) {
    $self->{value} = $value;
  }
}

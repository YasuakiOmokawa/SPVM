package SPVM::Int {
  has value : private int;
  
  sub new : SPVM::Int ($value : int) {
    my $self = new SPVM::Int;
    
    $self->{value} = $value;
    
    return $self;
  }
  
  sub get_value : int ($self : self) {
    return $self->{value};
  }

  sub set_value : void ($self : self, $value : int) {
    $self->{value} = $value;
  }
}

package Float {
  has value : private float;
  
  sub new : Float ($value : float) {
    my $self = new Float;
    
    $self->{value} = $value;
    
    return $self;
  }
  
  sub get_value : float ($self : self) {
    return $self->{value};
  }
  
  sub set_value : void ($self : self, $value : float) {
    $self->{value} = $value;
  }
}

package Long {
  has value : private long;
  
  sub new : Long ($value : long) {
    my $self = new Long;
    
    $self->{value} = $value;
    
    return $self;
  }
  
  sub get_value : long ($self : self) {
    return $self->{value};
  }

  sub set_value : void ($self : self, $value : long) {
    $self->{value} = $value;
  }
}
